-- by my implementation
lBool = true
    | false

-- all right recursion, stay
Atom =  lInteger 
      | lBool 
      | lVName 
      | KvGet Atom
      | Print Atom
      | KvPut Atom Atom
      | (Exp)

-- all right recursion, stay
FExp = Apply Atom Atom
    | Atom

LExp = Let lVName Exp Exp
        | TryCatch Exp Exp
        | Lambda lVName Exp
        | If Exp Exp Exp
        | FExp

Exp2 = ** Atom Exp2
        | ;

Exp1 =  * Atom Exp1
        | / Atom Exp1
        | ;

Exp0 =  + Atom Exp0
        |  - Atom Exp0
        | ;

Exp00 = == Exp0 Exp00
        | Atom Exp2 Exp1 Exp0

Exp = Exp00



